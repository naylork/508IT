<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EventsPlusLTD</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="C:\Users\KNayl\source\repos\EventsPlusLTD\EventsPlusLTD\css\bootstrap.min.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="C:\Users\KNayl\source\repos\EventsPlusLTD\EventsPlusLTD\js\bootstrap.min.js"></script>

</head>
<body>

    <nav class="navbar bg-dark navbar-dark">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><h1><b>EventsPlusLTD</b></h1></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="collapsibleNavbar">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Events" asp-action="Index">Event Information</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="TypeOfEvents" asp-action="Index">Type Of Event</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Performances" asp-action="Index">Performance</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="AssetsNeededs" asp-action="Index">Assets Needed</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Equipments" asp-action="Index">Equipment</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="EquipmentTypes" asp-action="Index">Equipment Types</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Participants" asp-action="Index">Participants</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Performers" asp-action="Index">Performers</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Act Types" asp-action="Index">Act Type</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="OrganizersOccupieds" asp-action="Index">Organizers Occupied</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="Organizers" asp-action="Index">Organizer</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="OrganizerInfoes" asp-action="Index">Organizer Info</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-area="" asp-controller="OrganizerRoles" asp-action="Index">Organizer Role</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <h2>@ViewBag.title</h2>

            </div>
        </div>
    </div>



    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - EventsPlusLTD - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
